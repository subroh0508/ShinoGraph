BASE <https://283db.org/resource/enum/>
PREFIX : <http://schema.org/>
PREFIX dbo: <http://dbpedia.org/ontology/>
PREFIX foaf: <http://xmlns.com/foaf/0.1/>
PREFIX owl: <http://www.w3.org/2002/07/owl#>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>

# カードのレアリティ
<Rarity> a rdfs:Class ;
    rdfs:label "レアリティ"@ja, "Rarity"@en, "レアリティ" ;
    rdfs:subClassOf owl:Thing ;
        owl:equivalentClass [
            a rdfs:Class ;
            owl:oneOf (
                <Rearity.R>
                <Rearity.SR>
                <Rearity.SSR>
            )
        ] .

<Rearity.R> a <Rarity>, owl:NamedIndividual ;
    rdfs:label "R" ;
    :name "レア"@ja, "R"@en, "R" .

<Rearity.SR> a <Rarity>, owl:NamedIndividual ;
    rdfs:label "SR" ;
    :name "Sレア"@ja, "SR"@en, "SR" .

<Rearity.SSR> a <Rarity>, owl:NamedIndividual ;
    rdfs:label "SSR" ;
    :name "SSレア"@ja, "SSR"@en, "SSR" .
