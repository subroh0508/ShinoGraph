{
  "name": "@shinograph/client",
  "version": "0.0.1",
  "main": "./build/cjs/index.js",
  "module": "./build/esm/index.js",
  "scripts": {
    "build": "npm-run-all -s build:cjs build:esm",
    "build:cjs": "tsc --project . --module commonjs --outDir ./build/cjs",
    "build:esm": "tsc --project . --module es2020 --outDir ./build/esm"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "@types/shinograph": "^0.0.1",
    "babel-loader": "^8.2.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "prettier-plugin-svelte": "^2.4.0",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  },
  "type": "module"
}
